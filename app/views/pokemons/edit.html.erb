<h1>Edit Pokemon</h1>

<%= form_for @pokemon do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :types %>
  <%= f.text_field :types %>
  <%= f.label :weight %>
  <%= f.text_field :weight %>
  <%= f.label :img_url %>
  <%= f.text_field :img_url %>

  <%= f.submit "Update Pokemon" %>
<% end %>
